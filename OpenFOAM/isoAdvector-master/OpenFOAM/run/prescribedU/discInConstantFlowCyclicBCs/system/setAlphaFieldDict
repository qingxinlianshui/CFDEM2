FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setAlphaFieldDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

field     "alpha.water";
type      cylinder;
direction (0 1 0);
radius    0.15;
centre    (.5 0 .75);

